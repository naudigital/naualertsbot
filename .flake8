[flake8]
max-line-length = 100
ignore =
    Q000, D100, D101, WPS404, WPS326, WPS305,
    WPS412, D105, WPS323, WPS226, WPS115,
    WPS237, WPS473, WPS465, WPS100, WPS202
max-awaits = 15
max-arguments = 10
max-methods = 15
max-local-variables = 10
max-imports = 20
max-expressions = 20
max-returns = 10
